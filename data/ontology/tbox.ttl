@prefix : <http://snu.ac.kr/dining/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://snu.ac.kr/dining/> rdf:type owl:Ontology .

#################################################################
#    Classes
#################################################################

:Venue rdf:type owl:Class ;
    rdfs:label "Dining Venue" ;
    rdfs:comment "A location that serves food." .

:MealService rdf:type owl:Class ;
    rdfs:label "Meal Service" ;
    rdfs:comment "A specific service period (Breakfast, Lunch, Dinner) at a Venue on a Date." .

:MenuItem rdf:type owl:Class ;
    rdfs:label "Menu Item" ;
    rdfs:comment "A specific food item offered during a Meal Service." .

#################################################################
#    Object Properties
#################################################################

:offers rdf:type owl:ObjectProperty ;
    rdfs:domain :Venue ;
    rdfs:range :MealService ;
    rdfs:label "offers" .

:providedAt rdf:type owl:ObjectProperty ;
    rdfs:domain :MealService ;
    rdfs:range :Venue ;
    owl:inverseOf :offers ;
    rdfs:label "provided at" .

:hasMenu rdf:type owl:ObjectProperty ;
    rdfs:domain :MealService ;
    rdfs:range :MenuItem ;
    rdfs:label "has menu item" .

:partOfService rdf:type owl:ObjectProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range :MealService ;
    owl:inverseOf :hasMenu ;
    rdfs:label "part of service" .

#################################################################
#    Data Properties
#################################################################

# Venue Properties
:venueId rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string .

:name rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string .

:placeName rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string .

:address rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string .

:phone rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string .

:building rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:string .

:floor rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:integer .

:geoLat rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:decimal .

:geoLng rdf:type owl:DatatypeProperty ;
    rdfs:domain :Venue ;
    rdfs:range xsd:decimal .

# MealService Properties
:date rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:date .

:mealType rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:string ;
    rdfs:comment "Breakfast, Lunch, Dinner" .

:timeRange rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:string ;
    rdfs:comment "Original time string e.g. '11:00~14:00'" .

:timeStart rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:time .

:timeEnd rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:time .

:description rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:string ;
    rdfs:comment "Raw description text" .

:serviceStyle rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:string ;
    rdfs:comment "Buffet, Standard, etc." .

:crowdTimeRange rdf:type owl:DatatypeProperty ;
    rdfs:domain :MealService ;
    rdfs:range xsd:string .

# MenuItem Properties
:menuName rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:string .

:price rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:integer .

:category rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:string ;
    rdfs:comment "Korean, Western, Noodle, etc." .

:tag rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:string ;
    rdfs:comment "Spicy, Takeout, etc." .

:consumptionMode rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:string ;
    rdfs:comment "Eat-in, Takeout" .

# New Classification Properties (LLM-based)
:cuisineType rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:string ;
    rdfs:comment "Korean, Western, Chinese, Japanese, etc." .

:containsMeat rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:boolean .

:carbType rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:string ;
    rdfs:comment "Rice, Noodle, Bread, None" .

:isSpicy rdf:type owl:DatatypeProperty ;
    rdfs:domain :MenuItem ;
    rdfs:range xsd:boolean .
