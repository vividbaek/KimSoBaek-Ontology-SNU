@prefix : <http://example.org/curriculum/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://example.org/curriculum/> rdf:type owl:Ontology ;
                                  rdfs:comment "Ontology for Curriculum, Subjects, and Competencies"@en .

#################################################################
#    Classes
#################################################################

:Subject a owl:Class ;
    rdfs:label "Subject" ;
    rdfs:comment "An educational course or subject." .

:JBNUSubject a owl:Class ;
    rdfs:subClassOf :Subject ;
    rdfs:label "JBNU Subject" ;
    rdfs:comment "Subject offered by JBNU." .

:COSSSubject a owl:Class ;
    rdfs:subClassOf :Subject ;
    rdfs:label "COSS Subject" ;
    rdfs:comment "Subject offered by COSS." .

:Competency a owl:Class ;
    rdfs:label "Competency/Skill" ;
    rdfs:comment "A skill or competency acquired through a subject." .

:TechnicalCompetency a owl:Class ;
    rdfs:subClassOf :Competency ;
    rdfs:label "Technical Competency" .

:SoftSkill a owl:Class ;
    rdfs:subClassOf :Competency ;
    rdfs:label "Soft Skill" .

:Track a owl:Class ;
    rdfs:label "Career Track" ;
    rdfs:comment "A career path or specialization track." .

:CurriculumDomain a owl:Class ;
    rdfs:label "Curriculum Domain" ;
    rdfs:comment "Broad domain of the subject (e.g., AI, SW)." .

#################################################################
#    Object Properties
#################################################################

:hasPrerequisite a owl:ObjectProperty, owl:TransitiveProperty ;
    rdfs:domain :Subject ;
    rdfs:range :Subject ;
    rdfs:label "has prerequisite" ;
    rdfs:comment "Indicates that a subject is a prerequisite for another." .

:teaches a owl:ObjectProperty ;
    rdfs:domain :Subject ;
    rdfs:range :Competency ;
    rdfs:label "teaches competency" ;
    rdfs:comment "Links a subject to the competencies it teaches." .

:requiresCompetency a owl:ObjectProperty ;
    rdfs:domain :Track ;
    rdfs:range :Competency ;
    rdfs:label "requires competency" ;
    rdfs:comment "Competencies required for a specific track." .

:relatedTo a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:domain :Subject ;
    rdfs:range :Subject ;
    rdfs:label "is related to" .

:equivalentTo a owl:ObjectProperty, owl:SymmetricProperty ;
    rdfs:subPropertyOf owl:sameAs ;
    rdfs:domain :Subject ;
    rdfs:range :Subject ;
    rdfs:label "is equivalent to" ;
    rdfs:comment "Indicates that two subjects are considered equivalent." .

:belongsToDomain a owl:ObjectProperty ;
    rdfs:domain :Subject ;
    rdfs:range :CurriculumDomain ;
    rdfs:label "belongs to domain" .

#################################################################
#    Data Properties
#################################################################

:hasTitle a owl:DatatypeProperty ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string ;
    rdfs:label "title" .

:hasSemester a owl:DatatypeProperty ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string ;
    rdfs:comment "e.g., '1-1', '2-2', 'Any'" .

:hasDescription a owl:DatatypeProperty ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string .

:offeredInSource a owl:DatatypeProperty ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string ;
    rdfs:comment "e.g., 'COSS', 'JBNU'" .

:hasDomain a owl:DatatypeProperty ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string ;
    rdfs:comment "String representation of domain (legacy support)." .

:difficultyLevel a owl:DatatypeProperty ;
    rdfs:domain :Subject ;
    rdfs:range xsd:string ;
    rdfs:comment "Difficulty level (e.g., Beginner, Intermediate)." .

:creditHours a owl:DatatypeProperty ;
    rdfs:domain :Subject ;
    rdfs:range xsd:integer .

#################################################################
#    Pre-defined Instances (Tracks)
#################################################################

:AIModeler a :Track ;
    rdfs:label "AI 모델러" ;
    :requiresCompetency :Competency_MachineLearning, :Competency_DeepLearning, :Competency_Python .

:DataEngineer a :Track ;
    rdfs:label "데이터 엔지니어" ;
    :requiresCompetency :Competency_Database, :Competency_BigData .
